            </div>
            <div class="col-xs-12 col-md-7">
                <!-- financial list of the page -->
                <ul class="list-unstyled financial-list">
                    @foreach (var item in ViewBag.section2)
                    {
                        <li>
                            <span class="icon ti-server"></span>
                            <div class="financial-list-detail">
                                <h3 class="heading3 fwRegular">@item.IconHeader</h3>
                                <p>It is a long established fact that a reader will be distracted by the readable content.</p>
                            </div>
                        </li>
                    }


                </ul>
            </div>
        </div>
    </section>
    <!-- choose sec of the page -->
    <section class="choose-sec bg-lightGrey">
        <div class="col pull-right">
            <div class="img-holder">
