<!-- main of the page -->
<main id="main">
    <!-- banner slide of the page -->
    <section class="banner-slide banner-sec">
        <!-- slide of the page -->
        <div class="slide">
            <div class="banner-holder bg-full" style="background-image: url(../Uploads/@ViewBag.topSection.Image)">
                <div class="container holder">
                    <div class="row">
                        <div class="col-xs-12 caption fwMedium text-center">
                            <h1 class="fwMedium">@ViewBag.topSection.Header</h1>
                            <p>@ViewBag.topSection.Description</p>
                            <a href="#" class="btn-primary text-uppercase">@ViewBag.topSection.btnText</a>
                        </div>
                    </div>
                </div>
            </div>
            <a href="#about" class="next-sec"></a>
        </div>
    </section>
    <!-- about sec of the page -->
    <section id="about" class="about-sec container">
        <div class="row">
            <div class="col-xs-12 col-md-5 header">
