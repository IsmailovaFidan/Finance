    <section class="project-sec bg-lightGrey">
        <div class="container">
            <div class="row">
                <div class="col-xs-12 header2 text-center">
                    <h2 class="heading fwExtra-Light text-capitalize">@ViewBag.ourproject.Header</h2>
                    <p>@ViewBag.ourproject.Description</p>
                </div>
            </div>
        </div>
        <!-- project holder of the page -->
        <div class="project-holder">
            <!-- project block of the page -->
            @foreach (var item in ViewBag.ourprojectPhoto)
            {
                <figure class="project-block overlay">
                    <img src="../Uploads/@item.Image" alt="image description" class="img-responsive">
                    <figcaption class="contant-holder bg-blue fwRegular white">
                        <h2 class="heading6"><a href="project-detail-fullwidth.html">@item.Header</a></h2>
                        <p>@item.Description</p>
